{
  "name": "electron-react-rspack",
  "version": "0.0.6",
  "author": "Ryan",
  "description": "An Electron boilerplate including TypeScript, React, Rspack and ESLint.",
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "typescript",
    "ts",
    "rspack"
  ],
  "main": "build/loader.js",
  "scripts": {
    "start": "pnpm --filter preload run build && pnpm --filter renderer run start",
    "build": "npm run clean && concurrently \"pnpm --filter main run build\" \"pnpm --filter preload run build\" \"pnpm --filter renderer run build\"",
    "clean": "ts-node ./scripts/clean.ts",
    "package:local": "npm run build && electron-builder build -c ./build.config.json --publish never",
    "package": "npm run build && electron-builder build -c ./build.config.json --publish always"
  },
  "devDependencies": {
    "@rsbuild/core": "^0.7.9",
    "@rspack/cli": "^0.7.4",
    "@rspack/core": "^0.7.4",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.14.8",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "axios": "^1.7.2",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-log": "^5.1.2",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.1.8",
    "eslint": "^8.50.0",
    "fs-extra": "^11.2.0",
    "lodash": "^4.17.21",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.15.7",
    "typescript": "^5.2.2"
  },
  "homepage": "https://github.com/RyanProMax/electron-react-rspack#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyanProMax/electron-react-rspack.git"
  },
  "devEngines": {
    "node": ">=20.x",
    "npm": ">=10.x"
  },
  "license": "MIT"
}
